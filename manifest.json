{
    "manifest_version": 3,
    "name": "Ez Images",
    "description": "Download an image by hovering over it's button!",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["Content.js"]
        }
    ],
    "icons": {
        "16": "/images/ez_images16.png",
        "32": "/images/ez_images32.png",
        "48": "/images/ez_images48.png",
        "128": "/images/ez_images128.png"
    }
}